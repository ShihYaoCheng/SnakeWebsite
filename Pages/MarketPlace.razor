    /// <summary>
    /// 
    /// </summary>
    private List<NFTData> NFTDataList;
    /// <summary>
    /// 顯示nft rider 筆數
    /// </summary>
    private int PPSRsCount;

    /// <summary>
    /// OpenSeaURL NFT資產 官方網址
    /// </summary>
    private string OpenSeaURL;

    protected override async Task OnInitializedAsync()
    {
