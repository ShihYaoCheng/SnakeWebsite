@inject IJSRuntime JS

<head>
    <link href="/css/NFTindex/NFTindex.css" rel="stylesheet" />
    <link href="/css/NFTindex/NFTindex_Gameplay.css" rel="stylesheet" />
</head>


<div class="NFTindex_Gameplay">
    <div class="NFTindex-Block-title">
        <span class="NFTindex-title" data-i18n="Home_data:gameplay.title">
            Gameplay
        </span>
    </div>
    <div class="NFTindex-subtitle" data-i18n="Home_data:gameplay.subtitle-01">Classic Solo Treasure Hunt</div>
    <div class="NFTindex-text" data-i18n="Home_data:gameplay.text-01">
        The Itza people have passed down these basic combat
        rules from ancient times. If you battle till
        the end, you can win the gold from your opponents. The combat may be attended by wild
        monsters, monsters, and wandering knights from the abyss, making it the most dangerous and
        valuable conflict to engage in.
    </div>

    <div class="Gameplay-mainImg-block-01">
        <img class="mainImg-01" src="/images/NFTindex/NFTindex_Gameplay/Diamond-01.png" alt="">
        <img class="mainImg-01" src="/images/NFTindex/NFTindex_Gameplay/Diamond-02.png" alt="">
    </div>
    <div class="Gameplay-mainImg-block-02">

        <div class="mainImg-02-block">
            <img class="mainImg-02" src="/images/NFTindex/NFTindex_Gameplay/Solo-01.png" alt="">
            <div class="mainImg-02-text" data-i18n="Home_data:gameplay.solo-01">1V7 Battle</div>
        </div>
        <div class="mainImg-02-block">
            <img class="mainImg-02" src="/images/NFTindex/NFTindex_Gameplay/Solo-02.png" alt="">
            <div class="mainImg-02-text" data-i18n="Home_data:gameplay.solo-02">Bet-on the victory</div>
        </div>
        <div class="mainImg-02-block">
            <img class="mainImg-02" src="/images/NFTindex/NFTindex_Gameplay/Solo-03.png" alt="">
            <div class="mainImg-02-text" data-i18n="Home_data:gameplay.solo-03">Rebirth Buff</div>
        </div>

    </div>


    <div class="NFTindex-subtitle" data-i18n="Home_data:gameplay.subtitle-02">quad Deathmatch</div>
    <div class="NFTindex-text" data-i18n="Home_data:gameplay.text-02">
        The Itza people have passed down these basic combat
        rules from ancient times. If you battle till
        the end, you can win the gold from your opponents. The combat may be attended by wild
        monsters, monsters, and wandering knights from the abyss, making it the most dangerous and
        valuable conflict to engage in.
    </div>
    <div class="Gameplay-mainImg-block-01">
        <img class="mainImg-01" src="/images/NFTindex/NFTindex_Gameplay/Skeleton-01.png" alt="">
        <img class="mainImg-01" src="/images/NFTindex/NFTindex_Gameplay/Skeleton-02.png" alt="">
    </div>

    <div class="Gameplay-mainImg-block-02">

        <div class="mainImg-02-block">
            <img class="mainImg-02" src="/images/NFTindex/NFTindex_Gameplay/Team-01.png" alt="">
            <div class="mainImg-02-text" data-i18n="Home_data:gameplay.team-01">4V4 Battle</div>
        </div>
        <div class="mainImg-02-block">
            <img class="mainImg-02" src="/images/NFTindex/NFTindex_Gameplay/Team-02.png" alt="">
            <div class="mainImg-02-text" data-i18n="Home_data:gameplay.team-02">E-sport Competition</div>
        </div>
        <div class="mainImg-02-block">
            <img class="mainImg-02" src="/images/NFTindex/NFTindex_Gameplay/Team-03.png" alt="">
            <div class="mainImg-02-text" data-i18n="Home_data:gameplay.team-03">Airship Support</div>
        </div>

    </div>


</div>