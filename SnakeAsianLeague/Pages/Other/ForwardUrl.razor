@page "/ForwardUrl/{Code}"
@using SnakeAsianLeague.Data.Services.ForwardUrl
@inject IJSRuntime JS
@inject ForwardUrlService ForwardUrlService

@code {
    [Parameter]
    public string? Code { get; set; } = "";

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        await ForwardUrlService.QRcodeSaveInvitation(Code);
        await JS.InvokeVoidAsync("signUpMobile",Code);
  
    }
}
